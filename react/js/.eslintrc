{
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true
  },
  "extends": ["plugin:react/recommended", "eslint:recommended", "prettier"],
  "plugins": ["react", "html", "prettier"],
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
